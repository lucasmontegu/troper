{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "^db:generate"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "dev": {
      "dependsOn": ["^dev", "^db:generate"],
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "dx": {
      "cache": false,
      "dependsOn": ["seed"]
    },
    "seed": {
      "cache": false,
      "dependsOn": ["migrate"]
    },
    "migrate": {
      "cache": false,
      "dependsOn": ["env"]
    },
    "env": {}
  }
}
