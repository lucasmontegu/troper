{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "lint": {},
    "dev": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "dx": {
      "cache": false,
      "dependsOn": ["seed"]
    },
    "seed": {
      "cache": false,
      "dependsOn": ["migrate"]
    },
    "migrate": {
      "cache": false,
      "dependsOn": ["env"]
    },
    "env": {}
  }
}
